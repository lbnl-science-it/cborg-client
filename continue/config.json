{

  "allowAnonymousTelemetry": false,

  "models": [
    {
      "title": "LBL/CBorgCoder",
      "provider": "openai",
      "model": "lbl/cborg-coder:latest",
      "contextLength": 131072,
      "apiBase": "http://127.0.0.1:8001",
      "apiKey": "none"
    },
    {
      "title": "OpenAI/ChatGPT",
      "provider": "openai",
      "model": "openai/chatgpt:latest",
      "contextLength": 131072,
      "apiBase": "http://127.0.0.1:8001",
      "apiKey": "none"
    },
    {
      "title": "Anthropic/Claude",
      "provider": "openai",
      "model": "anthropic/claude:latest",
      "contextLength": 131072,
      "apiBase": "http://127.0.0.1:8001",
      "apiKey": "none"
    },
    {
      "title": "Google/Gemini",
      "provider": "openai",
      "model": "google/gemini:latest",
      "contextLength": 131072,
      "apiBase": "http://127.0.0.1:8001",
      "apiKey": "none"
    }
  ],

  "requestOptions": {
    "headers": {
      "X-CBorg-Continue-ConfigVersion": 0.1
    }
  },

  "completionOptions": {
    "temperature": 0.0
  },

  "customCommands": [
    {
      "name": "test",
      "prompt": "{{{ input }}}\n\nWrite a comprehensive set of unit tests for the selected code. It should setup, run tests that check for correctness including important edge cases, and teardown. Ensure that the tests are complete and sophisticated. Give the tests just as chat output, don't edit any file.",
      "description": "Write unit tests for highlighted code"
    }
  ],

  "embeddingsProvider": {
    "provider": "openai",
    "model": "lbl/nomic-embed-text",
    "apiBase": "http://127.0.0.1:8002",
    "apiKey": "none"
  },

  "tabAutocompleteModel": {
    "provider": "openai",
    "model": "lbl/cborg-coder-base:latest",
    "apiBase": "http://127.0.0.1:8003",
    "contextLength": 131072,
    "apiKey": "none"
  },

  "tabAutocompleteOptions": {
    "debounceDelay": 750,
    "multilineCompletions": "always",
    "disableInFiles": ["*.csv", "*.txt", "*.md"]
  },

  "contextProviders": [
    {
      "name": "code",
      "params": {}
    },
    {
      "name": "docs",
      "params": {}
    },
    {
      "name": "diff",
      "params": {}
    },
    {
      "name": "terminal",
      "params": {}
    },
    {
      "name": "problems",
      "params": {}
    },
    {
      "name": "folder",
      "params": {}
    },
    {
      "name": "codebase",
      "params": {}
    }
  ],
  "slashCommands": [
    {
      "name": "edit",
      "description": "Edit selected code"
    },
    {
      "name": "comment",
      "description": "Write comments for the selected code"
    },
    {
      "name": "share",
      "description": "Export the current chat session to markdown"
    },
    {
      "name": "cmd",
      "description": "Generate a shell command"
    },
    {
      "name": "commit",
      "description": "Generate a git commit message"
    }
  ]
}
